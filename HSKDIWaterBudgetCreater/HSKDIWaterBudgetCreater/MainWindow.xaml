<Window x:Class="HSKDIWaterBudgetCreater.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Water Budget Calculator" Height="350" Width="525" SizeToContent="WidthAndHeight">
    <Grid>
        <TabControl Margin="32,30,0,0">
            <TabItem x:Name="Summary" Header="Summary">
                <Grid Background="#FFE5E5E5"/>
            </TabItem>
            <TabItem x:Name="LayerManage" Header="Manage Layers">
                <Grid x:Name="Layers" Background="#FFE5E5E5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="17*"/>
                        <ColumnDefinition Width="443*"/>
                    </Grid.ColumnDefinitions>
                    <Grid Grid.Column="1" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="437" Height="238"/>
                </Grid>
            </TabItem>
        </TabControl>

    </Grid>
</Window>
